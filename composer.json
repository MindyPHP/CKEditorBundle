{
    "name": "mindy/ckeditor-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@mindy-cms.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "egeloen/ckeditor-bundle": "~5.0 || ~6.0",
        "mindy/template-bundle": "~3.0 || ~4.0",
        "symfony/yaml": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/config": "~3.0 || ~4.0",
        "symfony/dependency-injection": "~3.0 || ~4.0"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\CKEditorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\CKEditorBundle\\Tests\\": ""
        }
    }
}
