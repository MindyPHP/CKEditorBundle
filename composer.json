{
    "name": "mindy/ckeditor-bundle",
    "type": "mindy-bundle",
    "homepage": "https://mindy-cms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@mindy-cms.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "egeloen/ckeditor-bundle": "~4.0||~5.0",
        "mindy/template-bundle": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/config": "~3.0",
        "symfony/dependency-injection": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\CKEditorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\CKEditorBundle\\Tests\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.6.8-dev"
        }
    }
}
